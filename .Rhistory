#loop through each row in the glass3 data set
getDistN <- function(training, q) {
distance <- numeric(nrow(training))
q <- as.numeric(unknownGlass1Tran[c(1:9)])
for (i in 1 : nrow(training)) {
p <- training[i, c(1:9)]
distance[i] <- getDist(p, q)
}
return(distance)
}
